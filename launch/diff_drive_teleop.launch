<?xml version="1.0"?>
<launch>
    <!-- This launches the Arduino differential drive ROS node -->
    <!-- <node pkg="rosserial_python" type="serial_node.py" name="serial_node">
        <param name="port" value="/dev/ttyACM0"/>
        <param name="baud" value="115200"/>
    </node> -->

    <node
        pkg="teleop"
        type="key_publisher.py"
        name="keyboard_driver"
        launch-prefix="xterm -e"
        respawn="true"
    />

    <node
        pkg="teleop"
        type="keys_to_twist.py"
        name="keys_to_twist"
        required="true"
    />

</launch>
